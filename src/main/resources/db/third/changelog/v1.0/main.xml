<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog logicalFilePath="main.xml" xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.4.xsd">

    <changeSet id="1.0" author="RinTheCat">
        <tagDatabase tag="1.0"/>
    </changeSet>

    <include file="init_tables.sql" relativeToChangelogFile="true"/>

    <changeSet id="import_companies" author="RinTheCat">
        <loadData
                schemaName="company"
                tableName="corporation"
                file="db/third/changelog/v1.0/data/companies.csv"
                separator=";"
        />
    </changeSet>

</databaseChangeLog>